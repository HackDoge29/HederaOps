{
  "name": "hederaops",
  "version": "1.0.0",
  "description": "Hedera services for HederaOps",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "start": "node dist/workflow.js",
    "dev": "ts-node src/workflow.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --runInBand --testPathPattern=integration",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@hashgraph/sdk": "^2.47.0",
    "dotenv": "^16.3.1",
    "solc": "^0.8.30"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.2"
  }
}
